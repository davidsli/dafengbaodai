<script>
    app.controller("createEnquiryProfileCtrl",
        [
            "$rootScope",
            "$scope",
            "$http",
            "$resource",
            "$stateParams",
            "$state",
            "$filter",
            function ($rootScope, $scope, $http, $resource, $stateParams, $state, $filter) {

                $scope.state_code = $stateParams.state_code;

                $scope.id = $stateParams.id;

                $scope.errorInfo = null;

                $scope.getProfile = function () {
                    $http.get("dafeng/getCreateEnquiryDetail?id=" + $scope.id).success(function(resResult){
                        $scope.createList = resResult.data;
                        console.log($scope.createList);
                    });
                };
                $scope.getProfile();


            }
        ]
    );

</script>

