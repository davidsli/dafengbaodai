<div class="vbox">
    <div class="wrapper-md b-b">
        {{entity_key}} /  workflowForm
    </div>
    <div class="row-row">
        <div class="cell scrollable">
            <div class="cell-inner">

                <div class="panel-body">

                    <form class="form-horizontal" method="get">
                        <div class="form-group">
                            <label class="col-sm-2 control-label">全名</label>
                            <div class="col-sm-10">
                                <input type="text" ng-model="workflow.label" class="form-control">
                                <span class="help-block m-b-none">全名</span>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-sm-4 col-sm-offset-2">
                                <a href="javascript:history.go(-1);" class="btn btn-default">取消</a>
                                <button type="button"  ng-click="save()" class="btn btn-primary">保存</button>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>
